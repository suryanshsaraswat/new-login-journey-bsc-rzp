{%- case flits_main_code -%}

{%- when 'module_profile' -%}
	<div class="flits-tab-box flits-profile-div" id="{{ id }}">
		{% render 'flits_capture_code' with 'component',template:'comp_tabsContainerHeader',text:navigation_profileText %} 
	</div>
{%- when 'module_profile_content' -%}
	<div class="flits-tab-box-body">
		<form action="/profile_save" id="flits-form-profile" method="post" class="flits-read-input">
			<div class="flits-row">
				<div class="flits-col-md-6 flits-mb-15">
					{% render 'flits_capture_code' with 'component',template:'comp_flitsLabelInput',
						type:'text', 
						label:profile_firstNameText, 
						value:customer.first_name,
						name: 'first_name',
						attr:'readonly'
					%} 
				</div>
			</div>
			<div class="flits-row">
				<div class="flits-col-md-6 flits-mb-15">
					{% render 'flits_capture_code' with 'component',template:'comp_flitsLabelInput',
						type:'text', 
						label:profile_lastNameText, 
						value:customer.last_name,
						name: 'last_name',
						attr:'readonly'
					%} 
				</div>
			</div>
			<div class="flits-row">
				<div class="flits-col-md-6 flits-mb-15">
					{% render 'flits_capture_code' with 'component',template:'comp_flitsLabelInput',
						type:'email', 
						label:profile_emailText, 
						value:customer.email,
						name: 'email',
						attr:'readonly'
					%}
				</div>
			</div>
			<div class="flits-row">
				<div class="flits-col-md-6 flits-mb-15">
					<div class="flits-phone-with-country-div flits-profile-phone-with-country-div flits-readonly-mode">
						<div class="flits-input-wrap">
							<label class="flits-input-label">{{ profile_contactNoText | strip }} : </label>
							<input type="tel" class="flits-input" name="phone" value="{{ customer.phone }}" readonly>
							<input type="text" class="flits-input flits-country-code-textbox" value="" disabled>
							<div class="flits-country-drpdown">
								<span class="flits-country-flag"></span>
								<div class="flits-select-row">
									<select class="flits-input"></select>
									<div class="flits-select-arrow">
										<svg viewBox="0 0 24 24" width="14" height="14" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" fill="none" shape-rendering="geometricPrecision">
											<path d="M6 9l6 6 6-6"></path>
										</svg> 
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="flits-row">
				<div class="flits-col-md-6 flits-mb-15" data-flits-birthdate="{{ customer.metafields.Flits.birthdate }}">
					{% render 'flits_capture_code' with 'component',template:'comp_flitsLabelInput',
						type:'text', 
						label:profile_birthdateText, 
						value:customer.metafields.Flits.birthdate,
						name: 'birthdate',
						attr:'min="1920-01-01" readonly data-clear-btn=true'
					%}
				</div>
			</div>
			<div class="flits-row">
				<div class="flits-col-md-6 flits-mb-15">
					<div class="flits-input-wrap">
						<label class="flits-input-label">{{ profile_genderText | strip }} : </label>
						<div class="flits-select-row">
							<select class="flits-input" name="gender" id="flits-profile-gender" disabled data-flits-gender="{{ customer.metafields.Flits.gender }}">
								<option value="none" class="flits-d-none"></option>
								<option value="male" {% if customer.metafields.Flits.gender == 'male' %}selected{% endif %}>{{ profile_maleText }}</option>
								<option value="female" {% if customer.metafields.Flits.gender == 'female' %}selected{% endif %}>{{ profile_femaleText }}</option>
								<option value="other" {% if customer.metafields.Flits.gender == 'other' %}selected{% endif %}>{{ profile_otherText }}</option>
							</select>
							<div class="flits-select-arrow">
								<svg viewBox="0 0 24 24" width="14" height="14" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" fill="none" shape-rendering="geometricPrecision">
									<path d="M6 9l6 6 6-6"></path>
								</svg> 
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="flits-row flits-d-none" id="flits-referby-row">
				<div class="flits-col-md-6 flits-mb-15 flits-read-input">
					{% render 'flits_capture_code' with 'component',template:'comp_flitsLabelInput',
						type:'text', 
						name: 'referby',
						label:refer_youAreReferredByText,
						attr:'readonly id="flits-referred-by"'
					%}
				</div>
			</div>
			{% render 'flits_capture_code' with 'component',template:'comp_button',
				type:'button',
				text:profile_buttonEditText,
				class:'flits-primary-btn flits-profile-edit-button flits-ml-0'
			%}
			<div class="flits-edit-form-btn-grp flits-d-none">
