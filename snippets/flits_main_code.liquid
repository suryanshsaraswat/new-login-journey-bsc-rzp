												<span class="flits-label">{{ order_paymentTypeText | strip }}</span>
												<span class="flits-value">{{ order.transactions[0].gateway | replace: '_',' ' | capitalize }}</span>
											</div>
										</div>
									</div>
								</div>
							</div> 
						{% endfor %}
					{% endpaginate %}
				{% endif %}
			</div>
			{% render 'flits_capture_code' with 'component',template:'comp_pagination', attr:'data-flits-pagination="flits-order"' %}
		</div>
