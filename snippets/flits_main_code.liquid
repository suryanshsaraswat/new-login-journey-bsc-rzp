														<span class="flits-bottom-full flits-ribbion flits-red">{{ order_productNotExistText | strip }}</span>
													</div>
                                                {% endunless %}
												</div>

												<div class="flits-item-deatils flits-left">
													{% if item.url and item.product.published_at %}
														<a href="{{ item.url }}" class="flits-link flits-item-title" data-tippy-content="{{ item.title | strip }}">
															{{ item.title | strip }}
														</a>
													{% else %}
														<a href="javascript:void(0);" class="flits-link flits-item-title" data-tippy-content="{{ order_productNotExistText | strip }}">
															{{ item.title | strip }}
														</a>
													{% endif %}
													<p class="flits-item-price">{{ item.price | money_without_trailing_zeros }}</p>
													{% unless item.variant.id %}
														<p>{{ order_productNotExistText | strip }}</p>
													{% endunless %}
												</div>
											</div>
