				<div class="flits-refer-credit-table-div flits-d-none">
					<p class="flits-h5 flits-credit-activity-title">{{ refer_creditsActivityText | strip }}</p>
					<p class="flits-h5 flits-credit-activity-title flits-mb-0">{{ refer_earnedCreditsText | strip }}: <span id="flits-refer-program-total-credits"></span></p>
					<ul class="flits-credit-log-list flits-credit-log-header">
						<li class="flits-credit-log-item flits-credit-log-title flits-refer-credit-log-item-template">
							<div class="flits-row flits-credit-log-details">
								<div class="flits-col-md-3 flits-credit-right-border">
									<p class="flits-referral-customer-name flits-text-nowrap">{{ refer_referralCustomerNameText | strip }} </p>
								</div>
								<div class="flits-col-md-3 flits-credit-right-border">
									<p class="flits-referral-customer-email flits-text-nowrap">{{ refer_referralCustomerEmailText | strip }} </p>
								</div>
								<div class="flits-col-md-3 flits-credit-right-border">
									<div>
									<div class="flits-credit-icon flits-earn-credit-icon-round flits-d-none">
										<svg viewBox="0 0 24 24" width="24" height="24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="css-i6dzq1"><circle cx="12" cy="12" r="10"></circle><line x1="12" y1="8" x2="12" y2="16"></line><line x1="8" y1="12" x2="16" y2="12"></line></svg>
									</div>
									<p class="flits-referral-customer-credit">{{ refer_creditsText | strip }} </p>
									</div>
								</div>
								<div class="flits-col-md-3">
								<p class="flits-referral-customer-credit-date">{{ refer_timeText | strip }} </p>
								</div>
							</div>
						</li>
					</ul>
					<ul class="flits-credit-log-list flits-refer-credit-log-list">
					</ul>
				</div>
				{% render 'flits_capture_code' with 'component',template:'comp_empty_section',text_1:refer_noHistoryYetText,text_2:refer_marketingLineText, class:'flits-d-none flits-refer-credit-empty flits-p-relative' %}
			</div>
		</div>
		{% render 'flits_capture_code' with 'component',template:'comp_flitsLoader', class:'flits-refer-credit-loader' %}
	</div>

{%- when 'module_changePassword' -%}
	<div class="flits-tab-box flits-change-password-div" id="{{ id }}">
		{% render 'flits_capture_code' with 'component',template:'comp_tabsContainerHeader',text:navigation_changePasswordText %}
	</div>
{%- when 'module_changePassword_content' -%}
	<div class="flits-tab-box-body">
		<form action="/update_password" id="flits-form-change-password" method="post" autocomplete="off">
			<div class="flits-row">
				<div class="flits-col-md-6 flits-mb-15">
					{% render 'flits_capture_code' with 'component',template:'comp_flitsLabelInput',
						type:'password',
						label:updatePassword_newPasswordText,
						name:'password',
						placeholder:updatePassword_newPasswordPlaceholderText,
						attr:'minlength="6" maxlength="40" autocomplete="new-password"'
					%}
					<small class="flits-hint flits-password-hint">{{ updatePassword_passwordHintMessageText | strip }}</small>
				</div>
			</div>
			<div class="flits-row">
				<div class="flits-col-md-6">
					{% render 'flits_capture_code' with 'component',template:'comp_flitsLabelInput',
						type:'password',
						label:updatePassword_confirmNewPasswordText,
						name:'password_confirmation',
						placeholder:updatePassword_confirmPasswordPlaceholderText,
						attr:'minlength="6" maxlength="40" autocomplete="new-password"'
					%}
				<small class="flits-hint flits-confirm-password-hint">{{ updatePassword_passwordHintMessageText | strip }}</small>
				</div>
			</div>
			{% render 'flits_capture_code' with 'component',template:'comp_button',
				type:'button',
				text:updatePassword_updatePasswordButtonText,
				class:'flits-primary-btn flits-change-pass-button flits-mt-15 flits-ml-0'
			%}
		</form>
	</div>
{%- endcase -%}
