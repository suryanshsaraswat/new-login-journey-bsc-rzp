									{% if shop.metafields.Flits.IS_ORDER_CONTACT_US_ENABLE and shop.metafields.Flits.IS_ORDER_CONTACT_US_ENABLE == 1 %}
										{% render 'flits_capture_code' with 'component', template:'contact_us_button', customer:customer, order:order class:'flits-secondary-btn'%}
