<script language="javascript" type="text/javascript">

var ssSessionId = '';
if(ssGetURLParameter( 'utm_source') != null) {
ssSessionId = ssGetURLParameter('utm_source');
}

var gclid= '';
if(ssGetURLParameter( 'gclid') != null) {
gclid= ssGetURLParameter('gclid');
}

if (ssSessionId.toLowerCase() == 'paidaffiliateoptimise') {
ssSetCookie(ssSessionId);
}
else if (ssSessionId.toLowerCase() != 'paidaffiliateoptimise' && ssSessionId.toLowerCase() !='' || gclid!='')

{
	if(checkCookie("paidaffiliateoptimise"))
	eraseCookie("paidaffiliateoptimise");
}

function readCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
}


function eraseCookie(name) {
  document.cookie = name + "=" + name + ";expires=Thu, 01 Jan 1970 00:00:00 GMT; path=/";
}

function checkCookie(name)
{
    return readCookie(name) != null;
}



function ssGetURLParameter(name) {
return decodeURIComponent((new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search)||[,""])[1].replace(/\+/g, '%20'))||null
}

function ssSetCookie(cValue) {
document.cookie = cValue + "=" + cValue + ";expires= " + ssCalculateExpireDate(07) +"; path=/";
}

function ssCalculateExpireDate (days) {
var d = new Date();
d.setTime(d.getTime() + (days*24*60*60*1000));
d.toUTCString();
return d;
}
</script>